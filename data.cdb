								{
									"id": "start26",
									"who": "BannerAnonymousGuard",
									"text": "You need to flee, your Highness! We'll cover you!",
									"isRight": true
								},
								{
									"id": "start27",
									"who": "BannerLion",
									"text": "But I must...",
									"isRight": false
								},
								{
									"id": "start28",
									"who": "BannerAnonymousGuard",
									"text": "You must run!",
									"isRight": true
								},
								{
									"id": "pursuit0",
									"who": "BannerLion",
									"text": "Quick! Where can I go?",
									"isRight": false
								},
								{
									"id": "pursuit1",
									"who": "BannerLion",
									"text": "They are coming right ahead! I am lost!",
									"isRight": false
								},
								{
									"id": "pursuit2",
									"who": "BannerLion",
									"text": "My only chance is to hide in the tall grass.",
									"isRight": false
								},
								{
									"id": "villager0",
									"who": "BannerAnonymousWorker",
									"text": "They are gone, my queen. But you should not stay in here!",
