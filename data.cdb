								{
									"id": "bifrost14",
									"who": "BannerBoar",
									"text": "You can't be serious...",
									"isRight": false
								},
								{
									"id": "additionalDialog0",
									"who": "BannerWolf",
									"text": "His power is absolute! Does it mean nothing to you?",
									"isRight": false
								},
								{
									"id": "bifrost15",
									"who": "BannerWolf",
									"text": "I won’t let you stand between me and Valhöll! Join me when you come to your senses.",
									"isRight": true
