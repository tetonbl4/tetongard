								{
									"id": "bifrost19",
									"who": "BannerRaven",
									"text": "What do you have in mind, Svarn?",
									"isRight": false
								},
								{
									"id": "bifrost20",
									"who": "BannerBoar",
									"text": "We will meet the remaining Aesir if we find the divine plain of Idavoll.",
									"isRight": false
								},
								{
									"id": "bifrost21",
									"who": "BannerRig",
									"text": "Do you mean we are to abandon Northgard?",
									"isRight": true
								},
								{
									"id": "bifrost22",
									"who": "BannerRaven",
									"text": "I'd gladly take a break from the sons of Muspell... not to mention Egil.",
									"isRight": false
								},
								{
									"id": "bifrost23",
									"who": "BannerBear",
									"text": "I won't let this demon or the sons of Muspell chase us.",
									"isRight": false
								},
								{
									"id": "bifrost24",
									"who": "BannerRig",
									"text": "How do you intend to stand against them?",
									"isRight": true
								},
								{
									"id": "bifrost25",
									"who": "BannerBear",
									"text": "There are other clans, now. I have allies.",
									"isRight": false
								},
								{
									"id": "bifrost26",
									"who": "BannerBoar",
									"text": "This will never suffice. Why not bring them along?",
									"isRight": false
								},
								{
									"id": "bifrost27",
									"who": "BannerBear",
									"text": "No. I won't abandon my land. And you shouldn't leave either.",
									"isRight": false
								},
								{
									"id": "bifrost28",
									"who": "BannerRaven",
									"text": "I have to try, Borgild... but I'll come back for you.",
									"isRight": false
								},
								{
									"id": "bifrost29",
									"who": "BannerBear",
									"text": "I can't promise I'll still be alive by then.",
									"isRight": false
								},
								{
									"id": "bifrost30",
									"who": "BannerBoar",
									"text": "We'll find a way to stop Ragnarök. You'll live.",
									"isRight": false
								},
								{
									"id": "bifrost31",
									"who": "BannerRig",
									"text": "And the sooner we depart, the quicker it will be.",
									"isRight": true
								},
								{
									"id": "bifrost32",
									"who": "BannerBoar",
									"text": "May the disir guide us.",
									"isRight": false
								},
								{
									"id": "bifrost33",
									"who": "BannerRig",
									"text": "Fair winds to all of you.",
									"isRight": true
								},
								{
									"id": "bifrost34",
									"who": "BannerRaven",
									"text": "Farewell, Borgild.",
									"isRight": false
								},
								{
									"id": "failure",
									"who": "BannerRig",
									"text": "Forgive me, my friends, for failing you...",
									"isRight": true
								},
								{
									"id": "failureHvedrung",
									"who": "BannerRig",
									"text": "With Hvedrung dead, our plan collapses.",
									"isRight": true
								},
								{
									"id": "muspell0",
									"who": "BannerBoar",
									"text": "The sons of Muspell are coming...",
									"isRight": false
								},
								{
									"id": "muspell1",
									"who": "BannerBear",
									"text": "We're used to them by now. We'll be alright.",
									"isRight": false
								},
								{
